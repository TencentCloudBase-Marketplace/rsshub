{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "rsshub",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-container",
        "inputs": {
          "mode": "low-cost",
          "serviceName": "rsshub",
          "servicePath": "/",
          "uploadType": "image",
          "containerPort": 1200,
          "imageInfo": {
            "imageUrl": "diygod/rsshub"
          },
          "envVariables": {}
        }
      }
    }
  }
}
